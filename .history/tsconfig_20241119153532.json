{ 
  "types": ["vite/client", "node"],
  "include": ["vite.config.*","vitest.config.*" , "env.d.ts"],

  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "types": ["vite-plugin-svgr/client"],
